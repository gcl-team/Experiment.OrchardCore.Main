{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenTelemetry": {
    "TempoUrl": "http://localhost:4318/v1/traces"
  },
  "MetricsOptions": {
    "FlushIntervalSeconds": 60,
    "Namespace": "Experiment.OrchardCore.Main/Performance",
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 2
  }
}
